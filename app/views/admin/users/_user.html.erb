<tr>
  <td>
    <%= link_to edit_admin_user_path(user), class: "btn btn-primary" do %>
      <em class="fa fa-pencil"></em>
    <% end %>
    <% if !current_user?(user) %>
      <%= link_to admin_user_path(user), method: :delete, class: "btn btn-danger",
        data: {confirm: "Bạn chắc chắn muốn xóa?"} do %>
        <em class="fa fa-trash"></em>
      <% end %>
    <% end %>
  </td>
  <td><%= user.name %></td>
  <td><%= user.email %></td>
</tr>
