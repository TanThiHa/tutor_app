<%= form_for [:admin, @subject] do |f| %>
  <%= render "shared/error_messages", object: @subject%>

  <div class="form-group">
    <%= f.text_field :name, class: "form-control", placeholder: "Tên môn học" %>
  </div>

  <div class="form-group">
    <%= f.submit yield(:button_text), class: "button-background" %>
  </div>
<% end %>
