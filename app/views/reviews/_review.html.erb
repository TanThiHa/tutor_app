<div class="col-md-12 review-item">
  <div>
    <span class="review-name">[<%= review.student.user.name %>]</span>
    <span class="review-content"><%= review.content %></span>
  </div>
  <div>
    <i class="rate_date"><%= review.created_at.strftime("%F %T") %> -</i>
    <% review.score.to_i.times do  %>
      <span class="fa fa-star checked"></span>
    <% end %>
    <% (5-review.score.to_i).times do  %>
      <span class="fa fa-star"></span>
    <% end %>
  </div>
</div>
